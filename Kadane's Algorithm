#Kadane approach
class Solution:
    def maxSubarraySum(self, arr):
        # Code here
        sums = 0
        maxSum = arr[0]
        for i in range(len(arr)):
            sums = sums+ arr[i]
            if sums > maxSum:
                maxSum = sums
            if sums < 0 :
                 sums =0
        return maxSum
            
